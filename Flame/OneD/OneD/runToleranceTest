#Run a convergence study
#inputs=H2/
inputs=gri3.0/
chemfile=$inputs"chem.inp"
thermfile=$inputs"therm.dat"
EndT=1e-2 #usually 1e-4 for all other experiments
#Method="EPIRK4"
Sam=2
Experiment=2

