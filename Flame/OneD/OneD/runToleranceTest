#ConFile="DataEPI2HydroSample1NoJacV4.txt"
Method="EPI3"
Tol=1e-10
relTol=1e-8
#absTol=1e-8 Vary this

./OneDIgn.x --chemfile=$chemfile --thermfile=$thermfile --StepSize=5e-3 --FinalTime=$EndT --MyFile=$ConFile  --KrylovTol=1e-4 --UseJac=1 --SampleNum=$Sam --Method=$Method --Experiment=$Experiment --relTol=$relTol --absTol=1e-4 --Delx=0
